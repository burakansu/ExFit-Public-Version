    <!-- page-wrapper Start-->
    <div class="page-wrapper" id="pageWrapper">
        <!-- Page Header Start-->
        <div class="page-main-header">
            <div class="main-header-right row m-0">
                <div class="main-header-left">
                    <div class="logo-wrapper"><a asp-action="Index" asp-controller="Home"><img class="img-fluid" src="/Logo/logo.png" alt=""></a></div>
                    <div class="dark-logo-wrapper"><a asp-action="Index" asp-controller="Home"><img class="img-fluid" src="/Logo/dark-logo.png" alt=""></a></div>
                    <div class="toggle-sidebar"><i class="status_toggle middle" data-feather="align-center" id="sidebar-toggle"></i></div>
                </div>

                <div class="nav-right col pull-right right-menu p-0">
                    <ul class="nav-menus">
                        <li><a class="text-dark" href="#!" onclick="javascript:toggleFullScreen()"><i data-feather="maximize"></i></a></li>
                        <li class="onhover-dropdown">
                            <div class="notification-box"><a asp-action="AllActivitiesToday" asp-controller="Personals"><i data-feather="bell"></i></a><span class="dot-animated"></span></div>
                            <ul class="notification-dropdown onhover-show-div">
                                <li>
                                    <p class="f-w-700 mb-0">Bildirimler<span class="pull-right badge badge-primary badge-pill">@Model.Tasks.Count</span></p>
                                </li>
                                @foreach (var item in Model.Tasks)
                                {
                                    @switch (@item.Description)
                                    {
                                        case "Yeni Üye Kayıt Eildi":
                                            <li class="noti-danger">
                                                <div class="media">
                                                    <a asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID">
                                                        <span class="notification-bg bg-light-danger"><i data-feather="user-check"> </i></span>
                                                        <div class="media-body">
                                                            <a asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID"><p>@item.Description</p></a><span>@item.Create_Date.ToString("dd/MM/yyyy")</span>
                                                        </div>
                                                    </a>
                                                </div>
                                            </li>
                                            break;
                                        case "Yeni Çalışan İşe Alındı":
                                            <li class="noti-secondary">
                                                <div class="media">
                                                    <span class="notification-bg bg-light-secondary"><i data-feather="check-circle"> </i></span>
                                                    <div class="media-body">
                                                        <a asp-action="_PersonalsDetails" asp-controller="Personals" asp-route-id="@item.User_ID"><p>@item.Description</p></a><span>@item.Create_Date.ToString("dd/MM/yyyy")</span>
                                                    </div>
                                                </div>
                                            </li>

                                            break;
                                        case "Yeni Egzersiz Programı":
                                            <li class="noti-primary">
                                                <div class="media">
                                                    <span class="notification-bg bg-light-primary"><i data-feather="activity"> </i></span>
                                                    <div class="media-body">
                                                        <p>@item.Description</p><span>@item.Create_Date.ToString("dd/MM/yyyy")</span>
                                                    </div>
                                                </div>
                                            </li>

                                            break;
                                        case "Analiz Sonucu":
                                            <li class="noti-success">
                                                <div class="media">
                                                    <span class="notification-bg bg-light-success"><i data-feather="file-text"> </i></span>
                                                    <div class="media-body">
                                                        <p>@item.Description</p><span>@item.Create_Date.ToString("dd/MM/yyyy")</span>
                                                    </div>
                                                </div>
                                            </li>
                                            break;
                                        case "Üye Güncellendi":
                                            <li class="noti-danger">
                                                <div class="media">

                                                    <span class="notification-bg bg-light-danger"><i data-feather="user-check"> </i></span>
                                                    <div class="media-body">
                                                        <a asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID"><p>@item.Description</p></a><span>@item.Create_Date.ToString("dd/MM/yyyy")</span>
                                                    </div>

                                                </div>
                                            </li>
                                            break;
                                        case "Üye Kaydı Pasif Durumda!":
                                            <li class="noti-danger">
                                                <div class="media">
                                                    <span class="notification-bg bg-light-danger"><i data-feather="user-check"> </i></span>
                                                    <div class="media-body">
                                                        <a asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID"><p>@item.Description</p></a><span>@item.Create_Date.ToString("dd/MM/yyyy")</span>
                                                    </div>
                                                </div>
                                            </li>
                                            break;
                                    }
                                }


                            </ul>
                        </li>
                        <li>
                            <div class="mode"><i class="fa fa-moon-o"></i></div>
                        </li>
                        <li class="onhover-dropdown p-0">
                            <button class="btn btn-primary-light" type="button"><a asp-controller="LogIn" asp-action="SignIn"><i data-feather="log-out"></i>Çıkış</a></button>
                        </li>
                    </ul>
                </div>
                <div class="d-lg-none mobile-toggle pull-right w-auto"><i data-feather="more-horizontal"></i></div>
            </div>
        </div>
        <!-- Page Header Ends -->
        <!-- Page Body Start-->
        <div class="page-body-wrapper horizontal-menu">
            <!-- Page Sidebar Start-->
            <header class="main-nav">
                <div class="sidebar-user text-center">

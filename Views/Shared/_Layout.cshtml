                                        <li><a>Egzersizlerin Hakkında</a></li>
                                    </ul>
                                </li>
                                <li class="sidebar-main-title">
                                    <div>
                                        <h6>Personel</h6>
                                    </div>
                                </li>
                                <li class="dropdown">
                                    <a class="nav-link menu-title" href="javascript:void(0)"><i data-feather="briefcase"></i><span>Personel</span></a>
                                    <ul class="nav-submenu menu-content">
                                        <li><a href="#" id="AddPersonal">Yeni Personel</a></li>
                                        <li><a href="#" id="PIndex">Personeller</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="right-arrow" id="right-arrow"><i data-feather="arrow-right"></i></div>
                    </div>
                </nav>
            </header>
            <!-- Page Sidebar Ends-->
            <div class="page-body">
                <!-- Container-fluid starts-->
                <div class="container-fluid">
                    <div id="ExFit_Body" class="row">


                        @RenderBody()


                    </div>
                </div>
                <!-- Container-fluid Ends-->
            </div>
            <!-- footer start-->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6 footer-copyright">
                            <p class="mb-0">Copyright 2022 © ExFit Tüm Hakları Saklıdır.</p>
                        </div>
                    </div>
                </div>
            </footer>
        </div>

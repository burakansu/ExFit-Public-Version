</div>
<div class="container-fluid dashboard-default-sec">
    <div class="row">
        <div class="col-sm-12 col-xl-12 col-lg-12 col-xxl-6 recent-order-sec">
            <div class="card">
                <div class="card-body">
                    <div class="table-responsive">
                        <h5>Son Eklenen Üyeler</h5>
                        <table class="table table-bordernone">
                            <thead>
                                <tr>
                                    <th>İsim</th>
                                    <th>Bitiş</th>
                                    <th>Mail</th>
                                    <th>
                                        <div class="setting-list">
                                            <ul class="list-unstyled setting-option">
                                                <li>
                                                    <div class="setting-primary"><i class="icon-settings"></i></div>
                                                </li>
                                                <li><i class="view-html fa fa-code font-primary"></i></li>
                                                <li><i class="icofont icofont-maximize full-card font-primary"></i></li>
                                                <li><i class="icofont icofont-minus minimize-card font-primary"></i></li>
                                                <li><i class="icofont icofont-refresh reload-card font-primary"></i></li>
                                                <li><i class="icofont icofont-error close-card font-primary"></i></li>
                                            </ul>
                                        </div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.LastMembers)
                                {
                                    <tr>
                                        <td>
                                            <img style="width:50px; height:50px; margin-right:15px;" class="img-50 img-fluid rounded-circle update_img_0" alt="" src="@item.IMG">
                                            <a asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID">@item.FullName</a>
                                        </td>
                                        <td><a asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID">@item.Registration_Time.ToString("MM/dd/yyyy")</a></td>
                                        <td><a asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID">@item.Mail</a></td>
                                        <td>
                                            <a asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID"><i data-feather="link-2"></i></a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <div class="code-box-copy">
                        <button class="code-box-copy__btn btn-clipboard" data-clipboard-target="#recent-order" title="Copy"><i class="icofont icofont-copy-alt"></i></button>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12 col-xl-12 col-lg-12 col-xxl-6 top-dealer-sec">
            <div class="card">
                <div class="card-header pb-0">
                    <div class="header-top d-sm-flex justify-content-between align-items-center">
                        <h5>Dönem Sonu Yaklaşanlar</h5>
                        <div class="center-content">
                            <p class="d-sm-flex align-items-center"><span class="m-r-10"></span></p>
                        </div>
                        <div class="setting-list">
                            <ul class="list-unstyled setting-option">
                                <li>
                                    <div class="setting-primary"><i class="icon-settings"></i></div>
                                </li>
                                <li><i class="view-html fa fa-code font-primary"></i></li>
                                <li><i class="icofont icofont-maximize full-card font-primary"></i></li>
                                <li><i class="icofont icofont-minus minimize-card font-primary"></i></li>
                                <li><i class="icofont icofont-refresh reload-card font-primary"></i></li>
                                <li><i class="icofont icofont-error close-card font-primary"> </i></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="card-body">
                    <div class="owl-carousel owl-theme" id="owl-carousel-14">
                        <div class="item">
                            <div class="row">
                                <div class="col-12">
                                    <div class="owl-carousel-16 owl-carousel owl-theme">
                                        @{
                                            int a = 0;
                                        }
                                        @foreach (var item in Model.Members)
                                        {
                                            @if (item.RemainingDay <= 30 && item.RemainingDay >= 0 && item.Block == 0)
                                            {
                                                <div class="item">
                                                    <div class="card">
                                                        <div class="top-dealerbox text-center">
                                                            <img style="width:100px; height:100px;" class="card-img-top" src="@item.IMG" alt="...">
                                                            <h6>@item.FullName</h6>
                                                            <p>@item.Mail</p>
                                                            <a class="btn btn-rounded" asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID"><strong>@item.RemainingDay</strong> Gün Kaldı!</a>
                                                        </div>
                                                    </div>
                                                </div>
                                                a = 1;
                                            }
                                            @if (item.RemainingDay <= 0 && item.Block == 0)
                                            {
                                                <div class="item">
                                                    <div class="card">
                                                        <div class="top-dealerbox text-center">
                                                            <img style="width:100px; height:100px;" class="card-img-top" src="@item.IMG" alt="...">
                                                            <h6>@item.FullName</h6>
                                                            <p>@item.Mail</p>
                                                            <a class="btn btn-rounded btn-secondary" style="color:white; !important" asp-action="OpenedMember" asp-controller="Members" asp-route-id="@item.Member_ID"><strong>@item.RemainingDay</strong> Gün Geçti!</a>
                                                        </div>
                                                    </div>
                                                </div>
                                                a = 1;
                                            }
                                        }
                                        @if (a == 0)
                                        {
                                            <div class="item">
                                                <div class="card">
                                                    <div class="top-dealerbox text-center">
                                                        <img style="width:100px; height:100px;" class="card-img-top" src="/images/avatarnull.png" alt="...">
                                                        <h6>ExFit - 2022</h6>
                                                        <p>-</p><a class="btn btn-rounded">+</a>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="code-box-copy">
                        <button class="code-box-copy__btn btn-clipboard" data-clipboard-target="#top-dealer" title="Copy"><i class="icofont icofont-copy-alt"></i></button>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


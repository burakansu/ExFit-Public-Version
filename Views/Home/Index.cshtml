                                {
                                    <h5>Normal</h5>
                                }
                                else
                                {
                                    <h5>Yoğun</h5>
                                }

                                <p>Sporcu Kapasitesi</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-3 col-lg-12 col-sm-12 box-col-3 chart_data_right second">
                    <div class="card income-card card-primary">
                        <div class="card-body">
                            <div class="round-progress knob-block text-center">
                                <div class="progress-circle">
                                    <input class="knob1" data-width="50" data-height="70" data-thickness=".3" data-fgcolor="#24695c" data-linecap="round" data-angleoffset="0" value="@Model.PersonalCapasity">
                                </div>
                                @if (@Model.PersonalCapasity < 80)
                                {
                                    <h5>Normal</h5>
                                }
                                else
                                {
                                    <h5>Yoğun</h5>
                                }
                                <p>Personel Kapasitesi</p>
                            </div>
                        </div>
                    </div>
                </div>
